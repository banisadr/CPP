#include <iostream>

using namespace std;

int main()
{
  long number = 20;
  bool yeee = false;
  while (yeee == false){
    for(int i=1; i<=10 ; i++){
      if(number%i!=0){
	yeee = false;
	number +=10;
	break;
      }
	yeee = true;
    }
    cout<<number<<endl;
  }
  cout<< number << endl;
  return 0;
  }
